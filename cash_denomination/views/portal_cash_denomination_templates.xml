<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="portal_my_home" name="Prtal Cash Denomination"
              customize_show="True"
              inherit_id="portal.portal_my_home" priority="100">
        <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <div class="o_portal_category row g-2 mt-3"
                 id="portal_common_category">
                <t t-call="portal.portal_docs_entry">
                    <t t-set="icon"
                       t-value="'/cash_denomination/static/src/img/portal-denomination.svg'"/>
                    <t t-set="title">Cash Denomination Records</t>
                    <t t-set="text">View, all the records of cash denomination in Portal
                    </t>
                    <t t-set="url" t-value="'/cash/denomination/portal'"/>
                    <t t-set="placeholder_count" t-value="'p_count'"/>
                </t>
            </div>
        </xpath>
    </template>
    <template id="portal_cash_denomination_template" name="Portal availability">
        <t t-call="portal.portal_layout">
            <div class="o_portal_category row g-2 mt-3"
                 id="portal_common_category">
            </div>
            <t t-set="breadcrumbs_searchbar" t-value="True"/>
            <t t-call="portal.portal_searchbar">
                <t t-set="title">Cash Denomination Records</t>
            </t>
            <form class="s_website_form form-horizontal container-fluid mt32">
                <label class="col-md-3 col-sm-4 control-label" for="name">
                    Cash Denomination Records
                </label>
                <div class="col-sm-4 col-md-6 o_website_cash_denomination_search">
                    <input type="text"
                           class="form-control o_portal_cash_denomination_input"
                           id="cash_denomination_input"
                           name="cash_denomination_search" required=""
                           placeholder="Search Reocrds with Person or Counter ..."/>
                    <div aria-label="Search" title="Search" clickable="1"
                         style="position: relative;left: 620px;top: -30px;">
                        <i class="fa fa-search" style="color: black;"/>
                    </div>
                </div>
                <div>
                    <div class="cash_denomination_results_table col-sm-4 col-md-6"/>
                </div>
            </form>
        </t>
    </template>
</odoo>
